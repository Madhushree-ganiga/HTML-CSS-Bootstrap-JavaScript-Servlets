  </servlet>
  
  <servlet>
  <servlet-name>benz</servlet-name>
  	<servlet-class>com.wolken.CarsDetails.BenzServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>kia</servlet-name>
