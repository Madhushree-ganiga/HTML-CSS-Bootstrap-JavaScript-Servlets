    <url-pattern>/MountainDev</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Frooti</servlet-name>
    <servlet-class>com.wolken.ColdDrinks.FrootiServlet</servlet-class>
  </servlet>
