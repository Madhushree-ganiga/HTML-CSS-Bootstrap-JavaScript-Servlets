    <servlet-class>com.wolken.ColdDrinks.FantaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Fanta</servlet-name>
    <url-pattern>/Fanta</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Lime</servlet-name>
    <servlet-class>com.wolken.ColdDrinks.LimeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Lime</servlet-name>
    <url-pattern>/Lime</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Maza</servlet-name>
